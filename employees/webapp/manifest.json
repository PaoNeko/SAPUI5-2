{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.logaligroup.employees",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.2",
      "toolsId": "917cc1ec-dff9-47cb-880f-8afe9e35e4b3"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.129.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.logaligroup.employees.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.logaligroup.employees.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMaster",
          "pattern": ":?query:",
          "target": "TargetMaster"
        },
        {
          "name": "RouteDetails",
          "pattern": "Employees({index})",
          "target":[
            "TargetMaster",
            "TargetDetails"
          ]
        }
      ],
      "targets": {
        "TargetContent": {
          "viewName": "Container"
        },
        "TargetMaster": {
          "parent": "TargetContent",
          "viewName": "Master",
          "controlId": "container-fcl",
          "controlAggregation": "beginColumnPages"
        },
        "TargetDetails": {
          "parent": "TargetContent",
          "viewName": "Details",
          "controlId": "container-fcl",
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "com.logaligroup.employees.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}